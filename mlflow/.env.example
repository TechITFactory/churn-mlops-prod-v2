# MLflow tracking server URL (e.g., http://mlflow.churn.svc.cluster.local:5000)
MLFLOW_TRACKING_URI=http://REPLACE-ME-MLFLOW-URL

# Artifact store (S3/minio)
MLFLOW_S3_ENDPOINT_URL=https://s3.amazonaws.com
MLFLOW_ARTIFACT_URI=s3://REPLACE-ME-YOUR-BUCKET/mlflow-artifacts

# Optional backend store (if running server yourself)
MLFLOW_BACKEND_STORE_URI=postgresql+psycopg2://mlflow:password@db-host:5432/mlflow

# AWS credentials (prefer IRSA on EKS instead of env vars)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=REPLACE
AWS_SECRET_ACCESS_KEY=REPLACE
